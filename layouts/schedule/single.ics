BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//school.brainhackmtl.org//schedule//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-WR-CALNAME:BrainHack School Calendar
X-WR-TIMEZONE:America/Toronto
{{- range .Site.Data.en.schedule.schedule -}}
{{- range .days -}}
{{- range .sessions }}
BEGIN:VEVENT
ORGANIZER;CN="BrainHack School"
DESCRIPTION:More details @ school.brainhack.org/schedule
UID:{{dateFormat "20060102T150405" .dateStart}}@school
DTSTAMP:{{dateFormat "20060102T150405Z" .dateStart}}
DTSTART;TZID=US-Eastern:{{dateFormat "20060102T150405" .dateStart}}
DTEND;TZID=US-Eastern:{{dateFormat "20060102T150405" .dateEnd}}
STATUS:CONFIRMED
SUMMARY:{{ .title | truncate 66}}
LOCATION:{{ $.Site.Data.en.schedule.linktoschedule }}
URL:{{ $.Site.Data.en.schedule.linktoschedule }}
END:VEVENT
{{- end -}}{{- end -}}{{- end }}
END:VCALENDAR
